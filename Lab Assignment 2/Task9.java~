//Task-9
public class Task9{
  public static void main(String[]args){
    int n = 5;
    int res = n*1;
    System.out.println("5 X  1 = "+res);
    res = n*2;
    System.out.println("5 X  2 = "+res);
    res = n*3;
    System.out.println("5 X  3 = "+res);
    res = n*4;
    System.out.println("5 X  4 = "+res);
    res = n*5;
    System.out.println("5 X  5 = "+res);
    res = n*6;
    System.out.println("5 X  6 = "+res);
    res = n*7;
    System.out.println("5 X  7 = "+res);
    res = n*8;
    System.out.println("5 X  8 = "+res);
    res = n*9;
    System.out.println("5 X  9 = "+res);
    res = n*10
    System.out.println("5 X  10 = "+res);
  }
}