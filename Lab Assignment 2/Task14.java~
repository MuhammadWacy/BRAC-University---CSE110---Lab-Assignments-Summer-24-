//Task-14
public class Task14{
  public static void main(String[]args){
    int a = 8;
    int b = 3;
    double side = Math.sqrt(((a/2)*(a/2)+(b*b)));
    double circumference = side*6;
    double triangle = (1/2)*8*3;
    double rectangle = 8*side;
    double area = (2*triangle)+rectangle;
   }
  }