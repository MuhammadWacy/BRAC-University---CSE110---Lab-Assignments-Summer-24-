import java.util.Scanner;

public class Task3{
  public static void main(String[]args){
    Scanner sc = new Scanner(System.in);
    
    System.out.println("Enter Number: ");
    int n = sc.nextInt();
    
    int count = 0;
    
    int j=1;
    while(n%2==0){
      if(n%j==0){
        count++;
      }
      j++;
    }
  }
}