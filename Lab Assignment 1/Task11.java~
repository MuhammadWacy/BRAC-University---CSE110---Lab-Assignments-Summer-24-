//Task-11
public class Task11{
  public static void main(String[]args){
  int a = 4.5;
  int b = 9.5;
  int c = Math.sqrt(4.5*4.5+9.5*9.5);
  double sinA = a/c;
  double cosA = b/c;
  double sinB = b/c;
  double cos B = a/c;
  System.out.println("sin(A) = "+sinA);
  System.out.println("cos(A) = "+cosA);
  System.out.println("sin(B) = "+sinB);
  System.out.println("cos(A) = "+cosA);
 }
}