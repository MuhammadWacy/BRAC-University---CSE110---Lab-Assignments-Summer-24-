//Task

import java.util.Scanner;

public class Taskwaste{
  public static void main(String[]args){
    Scanner sc = new Scanner(System.in);
    
    System.out.println("n = ");
    int num = sc.nextInt();
    int res = 0;
    int times = 1;
    
    int i = 0;
    while(i<num){
      if(res%3!=0 && res%5==0){
        res= 5*times;
        System.out.println(res);
        times++;
      }else if(res%5==0){
        times++;
      }
      i++;
    }
  }
}