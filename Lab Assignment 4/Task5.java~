//Task-5

import java.util.Scanner;

public class Task5{
  public static void main(String[]args){
    Scanner sc = new Scanner(System.in);
    
    System.out.print("Enter an integer: ");
    int n = sc.nextInt();
    int term = 1;
    int num;
    
    int i = 1;
    while(i<=n){
      num = sc.nextInt();
      System.out.println("Enter number "+term+":"+num);
      term++;
      i++;
    }
  }
}